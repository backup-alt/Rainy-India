{
  "projectName": "rainy-india",
  "projectVersion": "1.0",
  "projectType": "web",
  "nodeVersion": "18",
  "client": {
    "enabled": true,
    "source": "client"
  },
  "functions": {
    "enabled": true,
    "source": "functions",
    "targets": [
      "rainy_scraper",
      "api_handler",
      "cron_scraper"
    ]
  },
  "datastore": {
    "enabled": true
  },
  "cron": {
    "enabled": true,
    "targets": [
      "scraper_schedule"
    ]
  },
  "appSail": [
    {
        "source": "vision-service",
        "stack": "docker",
        "build_path": ".",
        "name": "vision",
        "command": "npm start",
        "env_variables": {},
        "memory": 1024
    }
  ]
}